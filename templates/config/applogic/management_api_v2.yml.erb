account:
  keychain:
    applogic:
      algorithm: RS256
      value: <%= @applogic_private_key %>

  jwt: {}

  actions:
    read_users:
      required_signatures: ['applogic']
      requires_account_totp: false

exchange:
  keychain:
    applogic:
      algorithm: RS256
      value: <%= @applogic_private_key %>

  jwt: {}

  actions:
    read_operations:
      required_signatures: ['applogic']
      requires_account_totp: false
